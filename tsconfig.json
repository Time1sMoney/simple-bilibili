{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/src",
    "types": ["chrome"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
